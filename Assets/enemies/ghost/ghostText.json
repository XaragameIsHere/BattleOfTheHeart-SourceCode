        "cutscene_Dialogue":
        [
            {
                "Start":
                [
                    
                    {
                        "Sprite": "Assets/Enemies/lesbianDuck/AnimationSprites/speaking.png",
                        "dialogueText": "Helloooooo",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Ghost?",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "This time I mean the big, scary, man who I wanna talk to and not my cat.",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Enemies/lesbianDuck/AnimationSprites/speaking.png",
                        "dialogueText": "Although Ghost the cat, if you’re there... with my spaceship that would be fine because I really don’t wanna go against this ghost the human guy",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Enemies/lesbianDuck/AnimationSprites/speaking.png",
                        "dialogueText": "Hey lady, do you know where this scary ghost guy is",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Why I certainly do!",
                        "Subject": false
                    }, 
                    {
                        "Sprite": "Assets/Enemies/lesbianDuck/AnimationSprites/speaking.png",
                        "dialogueText": "REALLY!? Can ya point me in his direction?",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Ghost: Of course I can. Well not really, I mean I can’t point you in HIS direction. Since Ghost isn’t a he. Ghost is... well... Ghost is a me.",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "...",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "It’s my voice isn’t it?",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Enemies/lesbianDuck/AnimationSprites/speaking.png",
                        "dialogueText": "God, I forget voice training one day and now the entire New West thinks I’m a guy.",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Un-fucking-believable",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "You fuck up one aspect of femininity and now all of a sudden you’re nothing. You’re a subhuman piece of mockery.",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Enemies/lesbianDuck/AnimationSprites/speaking.png",
                        "dialogueText": "I’m sorry, and I think your voice sounds nice",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Enemies/lesbianDuck/AnimationSprites/speaking.png",
                        "dialogueText": "Oh don’t bother flattering me. It’s been a long time since I’ve cared about any of that crap. Might as well call me a “he” or a “they” or whatever. At least it’s human.",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Enemies/lesbianDuck/AnimationSprites/speaking.png",
                        "dialogueText": "You know, you live as long as me and you start to realize none of this matters",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Goddamn death is a virtue",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "You are saying A LOT of bad words",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Let me ask you-",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "-Who led you here?",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "-Don’t answer",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "-Let me guess, Llewelyn the stupid gunslinger and that degenerate gambler Michael",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "You know, if they just accept the peaceful slumber of death",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Always fighting and for what?",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "To waste away in some bar. Suffering, for years.",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Look, if I could just grab my spaceship and my cat over there that’d be great...",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Oooohhhh, sorry can’t do that gorge. See I kinda have reputation to uphold",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Like if I let you go then I gotta let the next guy go and the next guy go. And like, the whole reason I’ve been cloned is to kill people",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "I mean, if I don’t spare people from the pain of life. Then the pain of my life is meaningless",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Why else would they keep cloning me",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Anyways, sorry kid but you know. It’s my namesake.",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "No one sees me and lives",
                        "Subject": false
                    }
                ],
                "End":
                [
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Why should I keep going there’s nothing left to do. Life just feels like I’m retreading the same path over and over again.",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "And killing people doesn’t feel that way?",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "I don’t know it… it feels meaningful you know",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "like I know I’m saving people from the meaninglessness of life",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "I’m doing what I was put on earth to do",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "I’m letting people rest",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "I suppose that’s one way of looking at it.",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "But the way I see it, you’re taking away people’s ability to experience the beautiful parts of life.",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Tell me this… uhh",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Leah… the name’s Leah",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Leah… why bother living when our whole lives revolve around a videogame?",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Huh?",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "The people who’ve been cloning me, they’re the developers of the game. We’re inside a videogame. Our existence is meaningless, we’re just scripted events in a game Leah.",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "why should we go on?",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "By killing these people I’m saving people from the pain of realizing their existence is meaningless",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "You’re only giving them false hope",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Pretending that we mean anything more in the universe than a bunch c# code in a compiler",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Ghost, my hope isn’t false, because my life isn’t false. I know my experiences are real. My feelings are real, and my life is real.",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "So what if it doesn’t have meaning, that only means the restrictions of happiness are gone.",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "If life has no meaning then we have true freedom to do what we want",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "see that’s the difference between you and me",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "We both drowned in the void, the emptiness, the meaninglessness, but when you fell in…",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "…you kept struggling, swimming towards some falsehood of meaning.",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Whereas I’ve found peace in drowning",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "but what experience do I have left? I’m tired, Leah. If my infinite life has no meaning then what do I do with it.",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Hmm...",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "...Well the developers keep cloning you because they expect you to kill, right?",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Yeah",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "So why don’t you just not kill, maybe they’ll stop cloning you",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "And what do I do if they don’t?",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Well maybe try finding what’s unique in yourself. Keep trying to discover yourself. And when that becomes boring try discovering other people",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "There’s whole planet of interesting people you have infinite time to get to know",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "What happens when they leave me?",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "When they die on me",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Then be happy, because you helped them experience life and now they have finally gone to rest",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Now you’ve given them two gifts.",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Maybe you can start with me",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "...",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "alright, you can have your cat back",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "...And your spaceship.",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Thank you",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "But you gotta promise me you’ll come back",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "I’ll be back Ghost",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "My name’s Natalie",
                        "Subject": false
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "I’ll be back Natalie",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Friends?",
                        "Subject": true
                    },
                    {
                        "Sprite": "Assets/Sprites/",
                        "dialogueText": "Friends",
                        "Subject": false
                    }
                ]
                
            }
